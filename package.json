  "private": true,
  "scripts": {
    "test": "node ./node_modules/nyc/bin/nyc ./node_modules/mocha/bin/mocha --require jsdom-global/register --require ignore-styles --require babel-polyfill --compilers js:babel-register \"test/**/*.test.js\"",
    "test-update-snapshots": "SNAPSHOT_UPDATE=1 node ./node_modules/nyc/bin/nyc ./node_modules/mocha/bin/mocha --require jsdom-global/register --require ignore-styles --require babel-polyfill --compilers js:babel-register \"test/**/*.test.js\"",
    "test-watch": "node ./node_modules/mocha/bin/mocha --require jsdom-global/register --require ignore-styles --require babel-polyfill --compilers js:babel-register --watch \"test/**/*.test.js\"",
    "test-mutation": "node ./node_modules/stryker/bin/stryker run",
    "json-server": "node ./node_modules/json-server/bin/index --routes mock-backend/routes.json  mock-backend/index.js",
    "build": "node ./node_modules/webpack/bin/webpack.js --env.mode=production --progress",
    "client-dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --env.mode=development --open",
    "client-prod": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --env.mode=production",
    "server": "nodemon ./bin/www",
    "start-dev": "concurrently \"npm run server\" \"npm run client-dev\"",
    "start-prod": "concurrently \"npm run server\" \"npm run client-prod\"",
    "start": "node ./bin/www"
  },
